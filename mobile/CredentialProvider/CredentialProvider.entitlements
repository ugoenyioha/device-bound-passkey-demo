<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- AutoFill Credential Provider - REQUIRED for credential provider extensions -->
    <key>com.apple.developer.authentication-services.autofill-credential-provider</key>
    <true/>

    <!-- Associated Domains - REQUIRED for credential provider extensions per Codex analysis
         Both the host app and extension need this for iOS to load the extension -->
    <key>com.apple.developer.associated-domains</key>
    <array>
        <string>webcredentials:passkeydemo.usableapps.local</string>
        <string>webcredentials:passkeydemo.usableapps.local?mode=developer</string>
        <string>webcredentials:localhost</string>
        <string>webcredentials:localhost?mode=developer</string>
    </array>

    <!-- App Groups - Share data with main app -->
    <key>com.apple.security.application-groups</key>
    <array>
        <string>group.com.demo.PasskeyDemo</string>
    </array>

    <!-- Keychain Access Groups - Share keychain with main app -->
    <key>keychain-access-groups</key>
    <array>
        <string>$(AppIdentifierPrefix)com.demo.PasskeyDemo</string>
    </array>
</dict>
</plist>
